<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="kioskLocator" class="com.clovisphere.example.KioskLocator"/>
    <bean id="storeLocator" class="com.clovisphere.example.StoreLocator" />
    <bean id="rentalService" class="com.clovisphere.example.RentalService">
        <constructor-arg ref="storeLocator"/>
    </bean>

</beans>